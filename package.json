{
  "name": "i18n-helper",
  "version": "0.0.0",
  "description": "Astro integration for help translating your site",
  "main": "./dist/i18n-helper.js",
  "module": "./dist/i18n-helper.mjs",
  "files": [
    "dist",
    "src/i18n-helper-app.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/i18n-helper.mjs",
      "require": "./dist/i18n-helper.js"
    },
    "./i18n-helper-app.ts": {
      "types": "./src/i18n-helper-app.ts",
      "import": "./src/i18n-helper-app.ts",
      "require": "./src/i18n-helper-app.ts"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite build --watch",
