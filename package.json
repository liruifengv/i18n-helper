  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.10.6",
    "typescript": "5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4"
  },
  "peerDependencies": {
    "astro": "^3.0.0 || ^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liruifengv/i18n-helper"
  },
  "bugs": {
    "url": "https://github.com/liruifengv/i18n-helper/issues"
  },
  "homepage": "https://github.com/liruifengv/i18n-helper",
